-@request.tests.each do |test|
  .row
    .col-md-1
    .col-md-2
      -if test.status == true
        .label.label-success
          Pass
      -elsif test.status == false
        .label.label-danger
          Fail
      -else 
        .label.label-default
          N/A
    .col-md-3
      =test.name
    .col-md-3
      =test.key
    .col-md-2
      =test.value
    .col-md-1